{
  "name": "app_example",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "tsx watch ./src/main.ts",
    "migration:create": "tsx ./src/scripts/migrations.ts create",
    "migration:run": "tsx ./src/scripts/migrations.ts run",
    "migration:revert": "tsx ./src/scripts/migrations.ts revert",
    "migration:show": "tsx ./src/scripts/migrations.ts show",
    "migration:run:prod": "node ./dist/scripts/migrations.js run",
    "migration:revert:prod": "node ./dist/scripts/migrations.js revert",
    "migration:show:prod": "node ./dist/scripts/migrations.js show",
    "seeder:list": "tsx ./src/scripts/seeders.ts list",
    "seeder:run": "tsx ./src/scripts/seeders.ts run",
    "seeder:clear": "tsx ./src/scripts/seeders.ts clear",
    "seeder:refresh": "tsx ./src/scripts/seeders.ts refresh",
    "seeder:list:prod": "node ./dist/scripts/seeders.js list",
    "seeder:run:prod": "node ./dist/scripts/seeders.js run",
    "seeder:clear:prod": "node ./dist/scripts/seeders.js clear",
    "seeder:refresh:prod": "node ./dist/scripts/seeders.js refresh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.15.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.0",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-type-provider-zod": "^6.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  }
}
